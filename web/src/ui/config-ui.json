{
    "header": {
        "icons": [
            {
                "iconId": "fr-icon-bank-fill",
                "linkProps": {
                    "href": "https://code.gouv.fr"
                },
                "text": "Code Gouv"
            }
        ],
        "menu": {
            "welcome": {
                "enabled": true
            },
            "catalog": {
                "enabled": true
            },
            "addSoftware": {
