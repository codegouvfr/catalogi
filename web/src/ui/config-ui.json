            "fields": {
                "keywords": true,
                "programmingLanguages": true,
                "applicationCategories": true,
                "softwareType": true
            }
        },
        "links": {
            "enabled": true
        },
        "userActions": {
            "enabled": true
        }
    },
    "footer": {
        "domains": [
            "info.gouv.fr",
            "service-public.fr",
            "legifrance.gouv.fr",
            "data.gouv.fr"
